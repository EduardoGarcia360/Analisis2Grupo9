@model List<Analisis2Grupo9.Models.TableViewsModels.EstadoTicketTableViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Estados Ticket</h2>
<div class="row">
    <div class="col-md-12">
        <input type="button" class="btn btn-primary" onclick="document.location.href = '@Url.Content("~/EstadoTicket/AddEstadoTicket")'" value="Nuevo" />
    </div>
</div>
<br />
<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <td>#</td>

                <td>Nombre</td>
                <td>Estado</td>
                <td>Acciones</td>


            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.idEstadoTicket</td>
                    <td>@oElemento.Nombre</td>
                    <td>@oElemento.Estado</td>
                    <td>
                        <input type="button" class="btn btn-danger" onclick="document.location.href= '@Url.Content("~/EstadoTicket/Edit?IdEstadoTicket="+oElemento.idEstadoTicket)'" value="Actualizar" />
                    </td>

                </tr>
            }
        </table>
    </div>
</div>
